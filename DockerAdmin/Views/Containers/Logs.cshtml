@model DockerAdmin.ViewModels.ContainerLogsViewModel
@{
    ViewData["Title"] = "Logs";
}

<h1 class="display-6">Container logs</h1>
<h6>Container id @Model.ContainerId</h6>

<pre style="background-color: #101010; color: #fff; height: 600px; max-height: 600px">
@Model.Logs
</pre>

<a class="btn btn-light btn-sm" asp-action="Index"> back </a>